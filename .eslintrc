{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "indent": ["error", 4],
    "import/first": 0,
    "no-unused-vars": 0,
    "comma-dangle": 0,
    "no-trailing-spaces": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "space-before-blocks": 0,
    "func-names": 0
  }
}